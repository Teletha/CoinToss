<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox id="chart" HBox.hgrow="ALWAYS" />
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <ComboBox id="market" minWidth="250.0" prefWidth="300.0">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </ComboBox>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <Label alignment="CENTER" prefWidth="50.0" text="開始日" />
                  <DatePicker id="startDate" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <Label alignment="CENTER" prefWidth="50.0" text="終了日" />
                  <DatePicker id="endDate" />
               </children>
            </HBox>
            <Button id="start" mnemonicParsing="false" text="Start">
               <VBox.margin>
                  <Insets />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</HBox>
